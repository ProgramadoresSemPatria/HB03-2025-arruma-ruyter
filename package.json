{
  "name": "arruma-ruyter-monorepo",
  "private": true,
  "scripts": {
    "dev:bot": "npm run build --prefix bot && npm run start --prefix bot",
    "dev:web": "npm run dev --prefix web-app",
    "dev:all": "npm-run-all --parallel dev:bot dev:web"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
